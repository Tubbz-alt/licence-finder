
<%= current_question do %>
  <%= form_tag sectors_path do %>
    <ul>
      <% @sectors.each do |sector| %>
        <li>
          <%= check_box_tag 'sector_ids[]', sector.public_id, false, :id => "sector_#{sector.public_id}" %>
          <%= label_tag "sector_#{sector.public_id}", sector.name %>
        </li>
      <% end %>
    </ul>
    <%= submit_tag 'Next step' %>
  <% end %>
<% end %>

<%= render partial: 'upcoming_questions' %>
