<%- if @completed_questions.present? -%>
  <div class="done-questions">
    <div class="lf--start-again"><%= link_to 'Start again', start_path, class: "govuk-link" %></div>
    <ol>
      <%- @completed_questions.each_with_index do |question_details, index| -%>
        <li class="done">
          <%= render "govuk_publishing_components/components/heading", {
            text: "#{index + 1}. #{question_details[0]}",
            padding: true,
            font_size: 19,
            heading_level: 3
          } %>          
          <div class="answer multiple">
            <ul>
              <% question_details[1].each do |answer| %>
                <li><%= answer.to_s %></li>
              <% end %>
            </ul>
          </div>
          <p class="undo"><%= link_to 'Change this answer', change_answer_url(question_details[2]), class: "govuk-link" %></p>
        </li>
      <%- end -%>
    </ol>
  </div>
<%- end -%>
